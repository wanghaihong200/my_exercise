        <artifactId>spring-boot-starter-parent</artifactId>
        <version>2.7.4</version>
        <relativePath/> <!-- lookup parent from repository -->
    </parent>
    <groupId>com.example</groupId>
    <artifactId>springboot-learn</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <name>springboot-learn</name>
    <description>springboot-learn</description>
    <properties>
        <java.version>1.8</java.version>
        <druid.version>1.2.13</druid.version>
        <mysql.version>8.0.30</mysql.version>
        <dynamic.datasource.boot.version>3.5.2</dynamic.datasource.boot.version>
